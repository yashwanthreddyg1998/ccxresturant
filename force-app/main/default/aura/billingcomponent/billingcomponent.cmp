<aura:component implements="flexipage:availableForAllPageTypes,lightning:isUrlAddressable" access="global">
   <aura:attribute type="id" name="order"/>
    <aura:attribute type="id" name="ordername"/>
    <aura:attribute type="Boolean" name="feedbackv" default="true"/>
    
    <aura:handler name="init" value="{!this}" action="{!c.init}"/>    
  
    <c:billingscreen order="{!v.order}" ordername="{!v.ordername}" feedbackv="{!v.feedbackv}"/>
   
     
</aura:component>