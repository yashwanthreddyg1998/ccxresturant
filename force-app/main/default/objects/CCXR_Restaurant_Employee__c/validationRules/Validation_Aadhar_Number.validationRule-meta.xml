<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_Aadhar_Number</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(CCXR_Aadhar_Number__c , &quot;^[0-9]{4}\\s[0-9]{4}\\s[0-9]{4}&quot;))</errorConditionFormula>
    <errorDisplayField>CCXR_Aadhar_Number__c</errorDisplayField>
    <errorMessage>Please Enter Valid Aadhar Number</errorMessage>
</ValidationRule>
